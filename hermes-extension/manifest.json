{
    "name": "Hermes Extension",
    "description": "Censor",
    "version": "1.0",
    "icons": {
      "128": "images/icon_128.png"
    },
    "browser_action": {
      "default_icon": "images/icon_16.png",
      "default_popup": "popup.html"
    },
    
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["content.js", "axios.min.js"]
      }
    ],

    "manifest_version": 2
  }